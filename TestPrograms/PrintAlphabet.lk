//output: abcdefhijklmnopqrstuvwxyz
fn main(args: string) returns int{
    char starting_character = 'a'; // if you change it to A you can get the uppercase alphabet
    print_alpha(starting_character); //change it to uppercase for uppercase alphabet
}

fn print_alpha(character: char) returns int{
    for(int i = 0; i < 26; step i, 1){
        int a = character + i; //automatically casts
        print("%c", a);
    }
}